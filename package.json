  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.8",
    "@types/openpgp": "^4.4.15",
    "@types/pbkdf2": "^3.1.0",
    "browserify": "^17.0.0",
    "nodemon": "^2.0.7",
    "tslint": "^6.1.3",
    "typescript": "^4.1.5"
