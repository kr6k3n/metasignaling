{
  "name": "metasignaling",
  "version": "1.0.0",
  "description": "metasignaling client for fws",
  "scripts": {
    "ts:build": "tsc",
    "start": "npm run ts:build",
    "dev": "nodemon --config ./nodemon.json -e ts,json",
    "bundle": "browserify dist/index.js -o bundle/bundle.js --fast",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
